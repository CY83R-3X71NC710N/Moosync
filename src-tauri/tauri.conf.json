{
  "app": {
    "macOSPrivateApi": false,
    "windows": [
      {
        "url": "/mainWindow",
        "height": 800,
        "width": 1024
      }
    ],
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": ["**/*"],
          "requireLiteralLeadingDot": false
        }
      },
      "csp": "default-src 'self'; img-src 'self' asset: media: https://asset.localhost; media-src 'self' asset: media: https://asset.localhost; connect-src ipc: http://ipc.localhost; styles-src 'unsafe-inline'",
      "dangerousDisableAssetCspModification": false,
      "freezePrototype": false,
      "pattern": {
        "use": "brownfield"
      }
    },
    "withGlobalTauri": true
  },
  "build": {
    "beforeBuildCommand": "CARGO_TARGET_DIR=./target-ui trunk build",
    "beforeDevCommand": "CARGO_TARGET_DIR=./target-ui trunk serve",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "externalBin": ["binaries/exthost"],
    "resources": [
      "bridge.js",
      "events.js",
      "setup-sandbox.js",
      "setup-node-sandbox.js"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": ["app", "deb", "dmg", "rpm", "msi", "nsis", "updater"],
    "linux": {
      "deb": {
        "desktopTemplate": "./moosync.desktop"
      }
    }
  },
  "identifier": "app.moosync.moosync",
  "plugins": {
    "deep-link": {
      "mobile": [
        { "host": "moosync.app", "pathPrefix": ["/youtube", "/spotify"] }
      ],
      "desktop": {
        "schemes": ["moosync"]
      }
    }
  },
  "productName": "Moosync",
  "version": "0.0.0"
}
