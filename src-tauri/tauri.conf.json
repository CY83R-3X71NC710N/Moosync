{
  "app": {
    "macOSPrivateApi": false,
    "windows": [
      {
        "url": "/mainWindow",
        "height": 800,
        "width": 1024
      }
    ],
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": [
            "**/*"
          ],
          "requireLiteralLeadingDot": false
        }
      },
      "csp": "default-src 'self'; img-src 'self' asset: media: https://asset.localhost; media-src 'self' asset: media: https://asset.localhost",
      "dangerousDisableAssetCspModification": true,
      "freezePrototype": false,
      "pattern": {
        "use": "brownfield"
      }
    },
    "withGlobalTauri": true
  },
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420/mainWindow",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": [
      "app",
      "appimage",
      "deb",
      "dmg",
      "rpm",
      "msi",
      "nsis",
      "updater"
    ],
    "linux": {
      "deb": {
        "desktopTemplate": "./moosync.desktop"
      }
    }
  },
  "identifier": "app.moosync.moosync",
  "plugins": {
    "deep-link": {
      "domains": [
        {
          "host": "moosync.app",
          "pathPrefix": [
            "/youtube"
          ]
        }
      ]
    }
  },
  "productName": "Moosync",
  "version": "0.0.0"
}